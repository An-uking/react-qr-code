{
  "name": "react-qr-code",
  "version": "0.1.1",
  "description": "A <QRCode /> component for React.",
  "keywords": [
    "react",
    "qr",
    "code",
    "QRCode"
  ],
  "author": {
    "name": "Rostyslav Khanas",
    "email": "rk@try.cards"
  },
  "scripts": {
    "storybook-web-build": "cd ./storybooks/web && npm run build-storybook",
    "storybook-web": "cd ./storybooks/web && npm run storybook",
    "storybook-native-watch": "babel ./src --out-dir=./storybooks/native/lib --watch",
    "storybook-native": "cd ./storybooks/native && npm run storybook",
    "storybook-emulator-ios": "cd ./storybooks/native && react-native run-ios",
    "storybook-ios": "npm run storybook-native-watch & npm run storybook-native & npm run storybook-emulator-ios",
    "clean": "rimraf lib",
    "lint": "eslint src",
    "test": "mocha --recursive --compilers js:babel-core/register",
    "build": "npm run lint && npm run clean && babel src --out-dir lib && npm run test",
    "release": "npm run build && npm run storybook-web-build"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:opensource-cards/react-qr-code.git"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "dependencies": {
    "qr.js": "0.0.0",
    "react-addons-shallow-compare": "^15.2.1"
  },
  "peerDependencies": {
    "react": "~15.3.2",
    "react-art": "^0.15.1",
    "react-native": "^0.38.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-allow-top-level-this": "0.0.1",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.6.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "jsdom": "^9.8.3",
    "jsdom-global": "^2.1.0",
    "mocha": "^3.2.0",
    "react": "~15.3.2",
    "react-art": "^0.15.1",
    "react-addons-test-utils": "~15.3.2",
    "react-dom": "~15.3.2",
    "react-native": "^0.38.0",
    "rimraf": "^2.5.4"
  }
}
